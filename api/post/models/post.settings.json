{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "name": "Post"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "content": {
      "type": "richtext"
    },
    "createdDate": {
      "type": "datetime"
    },
    "author": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "posts"
    }
  }
}
